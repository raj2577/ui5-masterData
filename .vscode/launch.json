      },
      "runtimeArgs": [
        "fiori",
        "run"
      ],
      "args": [
        "--config",
        "./ui5-mock.yaml",
        "--open",
        "test/flpSandbox.html#masterdetail-display"
      ],
      "console": "internalConsole",
      "internalConsoleOptions": "openOnSessionStart",
